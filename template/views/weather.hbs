<html>
    <head>
        <title>Weather Stats</title>
        <link rel="icon" href="/img/weather.png">
        <link rel="stylesheet" href="/css/style.css">
        <script src="/js/app.js"></script>
    </head>
    <body>
        <div class="main">
            {{>header}}
            <div class="weather">
                <p>{{error}}</p>
                <p>Location : {{location}}</p>
                <p>Forcast Data : {{forcast}}</p>
                <p>Address : {{address}}</p>
            </div>
            <p>Created By {{name}}</p>
        </div>
        {{>footer}}
    </body>
</html>
